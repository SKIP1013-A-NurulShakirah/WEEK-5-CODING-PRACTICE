package self_programming;

public class LogicalOperators {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		double income = 1000;
		int age = 40;
		double minLevel=500;
		
		if ((income > minLevel ) && (age <50))
		    System.out.println ("Can Apply Loan");
		
		if ((income == minLevel) || (age < 50));
		    System.out.println ("Can't Apply Loan");
		    
		System.out.print((income == minLevel) && (age < 50));
		 


	}

}
